package exersice3;

import java.util.Scanner;

public class FindFibonacciTerm {

	public static void main(String[] args) {
		
		Scanner scn= new Scanner(System.in);
		System.out.println("Enter the term you want ");

	}
	
	public static int fibbo(int n)
    {
        int a = 1, b = 1, c;
        if(n==0)
        	return 0;
        if (n == 1)
            return a;
        for (int i = 2; i <= n; i++) {
            c = a + b;
            a = b;
            b = c;
        }
        return b;
    }

}
