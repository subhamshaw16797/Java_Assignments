package exersice4;

import java.util.Iterator;
import java.util.Scanner;

public class PrimeNumbers {

	public static void main(String[] args) {
		
		Scanner scn=new Scanner(System.in);
		System.out.println("Enter range for prime number: ");
		int n= scn.nextInt();
		System.out.println("List of prime number between 1 to "+n+" : ");
		System.out.print(primeNum(n));
	}
	
	static int primeNum(int n) {
		
		for(int i=1; i<=n; i++) {
			if(i<=1)
				continue;
			boolean flag=true;
			for(int j=2; i<=i/2;j++) {
				if(i%2==0)
					flag=false;
			}
		}
	}

}
